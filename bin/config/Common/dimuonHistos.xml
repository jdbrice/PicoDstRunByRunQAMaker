		<Histo name="like_sign_Neg" title="like sign neg; M_{#mu#mu} [GeV/c^{2}]; dN/dM [c^{2}/GeV]" bins_x="dimuonBins.invMass" />

		<!-- 1D for pT slices computed at runtime-->
		<Histo name="like_sign_pT_{min}_to_{max}" title="like sign;  M_{#mu#mu} [GeV/c^{2}]; dN/dM [c^{2}/GeV]" bins_x="dimuonBins.invMass" />
		<Histo name="unlike_sign_pT_{min}_to_{max}" title="unlike sign;  M_{#mu#mu} [GeV/c^{2}]; dN/dM [c^{2}/GeV]" bins_x="dimuonBins.invMass" />
		<Histo name="like_sign_Pos_pT_{min}_to_{max}" title="like sign pos;  M_{#mu#mu} [GeV/c^{2}]; dN/dM [c^{2}/GeV]" bins_x="dimuonBins.invMass" />
		<Histo name="like_sign_Neg_pT_{min}_to_{max}" title="like sign neg;  M_{#mu#mu} [GeV/c^{2}]; dN/dM [c^{2}/GeV]" bins_x="dimuonBins.invMass" />

