			<Histo name="gmBg_pT_vs_m" title="B_{+-}/#sqrt{B_{++}+B_{--}}" bins_x="dimuonBins.invMass" bins_y="dimuonBins.pT"/>

			<Histo name="am_acorr_pT_vs_m" title="B_{+-}/(B_{++}+B_{--})" bins_x="dimuonBins.invMass" bins_y="dimuonBins.pT" />
