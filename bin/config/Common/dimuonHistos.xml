			<Histo name="signal_over_bg" title="S/B" bins_x="dimuonBins.invMass" />
			<Histo name="signal_over_sqrt_signal_bg" title="S/#sqrt{S+B}" bins_x="dimuonBins.invMass" />
			<Histo name="signal_over_sqrt_signal_2bg" title="S/#sqrt{S+2B}" bins_x="dimuonBins.invMass" />
			<Histo name="signal_bg" title="S+B" bins_x="dimuonBins.invMass" />
			<Histo name="sqrt_signal_bg" title="#sqrt{S+B}" bins_x="dimuonBins.invMass" />
			<Histo name="sqrt_signal_2bg" title="#sqrt{S+2B}" bins_x="dimuonBins.invMass" />
		</Plots>

		<Plots dir="mixedEvent">
			<Histo name="am_acorr" title="B_{+-}/(B_{++}+B_{--})" bins_x="dimuonBins.invMass" />
