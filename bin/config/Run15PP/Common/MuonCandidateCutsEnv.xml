		<Range name="dzMtd" min="{TRACK_CUT_dzMtd_min}" max="{TRACK_CUT_dzMtd_max}" />
		<Range name="dyMtd" min="{TRACK_CUT_dyMtd_min}" max="{TRACK_CUT_dyMtd_max}" />
		<Range name="drMtd" min="{TRACK_CUT_drMtd_min}" max="{TRACK_CUT_drMtd_max}" />
		
		<Range name="dTofMtd" min="{TRACK_CUT_dTofMtd_min}" max="{TRACK_CUT_dTofMtd_max}" />
	</MuonCandidateCuts>
