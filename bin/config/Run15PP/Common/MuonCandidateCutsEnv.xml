		<Range name="nHitsFit" min="{TRACK_CUT_nHitsFit_min}" />
		<Range name="eta" min="{TRACK_CUT_eta_min}" max="{TRACK_CUT_eta_max}" />
		
		<Range name="pt" min="{TRACK_CUT_pt_min}" />
		<Range name="nSigmaPion" min="{TRACK_CUT_nSigmaPion_min}" max="{TRACK_CUT_nSigmaPion_max}" />
