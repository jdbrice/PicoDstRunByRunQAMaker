<?xml version="1.0" encoding="UTF-8"?>
<config>

	<Task name="PicoDstQAMaker" type="RunByRunQAMaker" config="" nodePath="RunByRunQAMaker" />

	<Logger color="true" globalLogLevel="info" />
	

	<RunByRunQAMaker event="true" track="false" noRunMap="true">
		<Logger color="true" globalLogLevel="info" />

		<input>
			<dst treeName="PicoDst" url="list.lis" maxFiles="10000" nEvents="2000"/>
		</input>

		<output path="qa/" >
			<data>qa.root</data>
		</output>

		<Include url="config/histograms.xml" />

		<TrackCuts>
			<MtdMatch>true</MtdMatch>
		</TrackCuts>

	</RunByRunQAMaker>

	<Include url="config/bins.xml" />

</config>